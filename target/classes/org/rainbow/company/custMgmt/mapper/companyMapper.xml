   <!-- 기업 정보  가져오기-->
  <select id="companyView" resultType="org.rainbow.company.custMgmt.domain.companyVO" parameterType="int">
  	select * from RAIN_cCompany_tbl where companyNo=#{companyNo}
  </select>
  
  
  
