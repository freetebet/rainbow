<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="org.rainbow.company.custMgmt.mapper.salesMapper">
 
 
 

<!-- 상담 요청 리스트 가져오기-->
<select id="getSalesList" resultType="org.rainbow.company.custMgmt.domain.consultVO" parameterType="org.rainbow.domain.Criteria">
   
    SELECT consultNo, csDate, csCompanyName, csName, csContact, csEmail, csBdgt, csStatus, csEname
    FROM RAIN_consult_tbl ORDER BY consultNo
   
</select>

 <!-- 상담 요청 내용 가져오기-->
  <select id="getSalesView" resultType="org.rainbow.company.custMgmt.domain.consultVO" parameterType="int">
  	select * from RAIN_consult_tbl where consultNo=#{consultNo}
  </select>
 
 
 
 </mapper>